        if (minEnrage >= 30000) return 'bg-purple-100 dark:bg-purple-900/30 text-purple-800 dark:text-purple-300';
        if (minEnrage >= 25000) return 'bg-red-100 dark:bg-red-900/30 text-red-800 dark:text-red-300';
        if (minEnrage >= 20000) return 'bg-orange-100 dark:bg-orange-900/30 text-orange-800 dark:text-orange-300';
        if (minEnrage >= 15000) return 'bg-yellow-100 dark:bg-yellow-900/30 text-yellow-800 dark:text-yellow-300';
