    // Helper function to get bracket progress color
    function getBracketProgressColor(minEnrage) {
        if (minEnrage >= 30000) return 'bg-purple-600';
        if (minEnrage >= 25000) return 'bg-red-600';
        if (minEnrage >= 20000) return 'bg-orange-600';
        if (minEnrage >= 15000) return 'bg-yellow-600';
        if (minEnrage >= 10000) return 'bg-green-600';
        if (minEnrage >= 5000) return 'bg-blue-600';
        return 'bg-gray-600';
    }

    // Helper function to calculate bracket progress (simplified)
    function calculateBracketProgress(bracket) {
        // This is a simplified progress calculation - you might want to adjust this
        if (bracket.kills === 0) return 0;
        
        // More kills = more progress, capped at 100%
        const progress = Math.min(bracket.kills * 10, 100);
        return progress;
    }

